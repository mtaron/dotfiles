{
    "terminal.integrated.fontFamily": "'Cascadia Mono PL', 'FontAwesome'",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "explorer.confirmDelete": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "yaml.validate": false,
    "redhat.telemetry.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "editor.renderWhitespace": "boundary",
    "editor.bracketPairColorization.enabled": true,
    "diffEditor.renderSideBySide": false,
    "editor.fontFamily": "'Cascadia Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "files.autoSave": "afterDelay",
    "workbench.colorTheme": "GitHub Dark Default",
    "dotfiles.repository": "https://github.com/mtaron/dotfiles.git",
    "telemetry.telemetryLevel": "error",
    "editor.guides.bracketPairs": true,
    "svelte.enable-ts-plugin": true,
    "python.terminal.activateEnvironment": false,
    "window.titleBarStyle": "custom",
    "terminal.integrated.shellIntegration.showWelcome": false,
    "terminal.integrated.allowChords": false,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "editor.tabCompletion": "on",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "none",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.scrollback": 4000
}
